		{
			"label": "Sync with master and push",
			"type": "shell",
			"command": "git fetch origin master && git merge origin/master && git push origin copilot/add-mute-function-realtime-agent",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Complete merge and push",
			"type": "shell",
			"command": "git commit -m \"Merge master into copilot/add-mute-function-realtime-agent\" && git push origin copilot/add-mute-function-realtime-agent",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Check git status",
			"type": "shell",
			"command": "git status",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Complete merge and push",
			"type": "shell",
			"command": "git add -A && git commit --no-edit && git push origin copilot/add-mute-function-realtime-agent",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Complete merge",
			"type": "shell",
			"command": "git commit --no-edit && git push origin copilot/add-mute-function-realtime-agent",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Push branch",
			"type": "shell",
			"command": "git push origin copilot/add-mute-function-realtime-agent",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Merge PR",
			"type": "shell",
			"command": "gh pr merge 9 --squash --delete-branch",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Ready and merge PR",
			"type": "shell",
			"command": "gh pr ready 9 && gh pr merge 9 --squash --delete-branch",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Check PR status",
			"type": "shell",
			"command": "gh pr view 9",
			"isBackground": false,
			"group": "build"
		},
		{
			"label": "Check git state",
			"type": "shell",
			"command": "git status && git log --oneline -5",
			"isBackground": false,
